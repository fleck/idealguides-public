{
  "name": "idealguides",
  "version": "1.0.0",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:main": "node serverDev.js",
    "dev:indexer": "export NODE_ENV=development && cd packages/indexer/ && npm i && npm run build && npm start",
    "dev:embed": "yarn test:setup:build-embed --watch",
    "build": "blitz build",
    "start": "PORT=8080 node server.js",
    "start:db": "pg_ctl -D /usr/local/var/postgres start",
    "start:indexer": "cd packages/indexer/ && npm start",
    "studio": "blitz prisma studio",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "export NODE_ENV=test APP_ENV=test PORT=8080 && run-p test:setup:* && start-server-and-test start 8080 start:indexer http://localhost:8080/api/indexers/connected test-and-collect",
    "test:debug": "export NODE_ENV=test APP_ENV=test DATABASE_URL=postgresql://jfleckenstein@localhost:5432/idealguides_test && run-p test:setup:* && yarn start",
    "test:debug:dev": "export NODE_ENV=test APP_ENV=test PORT=3000 && run-p test:setup:db && run-p dev:*",
    "prepare": "husky install",
    "test:setup:build": "blitz build",
    "test:setup:build-embed": "npx tailwindcss -c ./tailwind-embed.config.js -i ./app/core/styles/embed.css -o ./public/embed.css",
    "test:setup:build-indexer": "cd packages/indexer/ && npm i && npm run build",
    "test:setup:db": "echo \"Y\" | blitz prisma migrate reset --force",
    "test:setup:clear-coverage": "rm -rf coverage && rm -rf .nyc_output",
    "test-and-collect": "playwright test && curl http://localhost:8080/api/coverage --output .nyc_output/blitz_coverage.json",
    "coverage": "nyc report --reporter=html && open ./coverage/index.html"
  },
  "prisma": {
    "schema": "db/schema.prisma",
    "seed": "ts-node -T --compiler-options {\"module\":\"CommonJS\"} ./db/seeds.ts"
  },
  "license": "UNLICENSED",
  "prettier": {
    "semi": false,
    "trailingComma": "all"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
